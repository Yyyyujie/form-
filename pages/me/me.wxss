/* pages/me/me.wxss */
.headBox{
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 30rpx 0;
  box-sizing: border-box;
}
.headImg{
  width: 150rpx;
  height: 150rpx;
  margin: 0 auto 20rpx;
  border-radius: 50%;
  overflow: hidden;
  
}
.headImg image{
  width: 100%;
  height: 100%;
}
.cellBox{
  padding: 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.cell{
  padding: 26rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cellTitle{
  display: flex;
  align-items: center;
}
.cellTitle .iconfont{
  margin-right: 10rpx;
}
.more{
  color: #ccc
}